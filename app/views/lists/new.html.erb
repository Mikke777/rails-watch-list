<div class="container d-flex justify-content-center mt-5">
  <div class="col-4">
    <h1>Create your new list!</h1>
    <%= simple_form_for [@list] do |f| %>
      <%= f.input :name, label: "List Name:" %>
      <%= f.input :photos, as: :file %>
      <%= f.submit "Submit new List", class: "button-new-list" %>
    <% end %>
  </div>
</div>
